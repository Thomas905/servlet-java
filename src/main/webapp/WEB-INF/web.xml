<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>servlet</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>HelloWorld</display-name>
    <servlet-name>HelloWorld</servlet-name>
    <servlet-class>com.nextu.app.servlets.HelloWorld</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloWorld</servlet-name>
    <url-pattern>/helloworld</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CreationProduit</display-name>
    <servlet-name>CreationProduit</servlet-name>
    <servlet-class>com.nextu.app.servlets.CreationProduit</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreationProduit</servlet-name>
    <url-pattern>/creationProduit</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CreationUsers</display-name>
    <servlet-name>CreationUsers</servlet-name>
    <servlet-class>com.nextu.app.servlets.CreationUsers</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreationUsers</servlet-name>
    <url-pattern>/creationUsers</url-pattern>
  </servlet-mapping>
</web-app>